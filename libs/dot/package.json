{
	"name": "@mscharley/dot",
	"version": "2.2.3",
	"publishConfig": {
		"provenance": true
	},
	"description": "A lightweight inversion of control framework for JavaScript and TypeScript",
	"engines": {
		"node": ">= 18"
	},
	"keywords": [
		"node",
		"javascript",
		"typescript",
		"dependency injection",
		"di",
		"ioc"
	],
	"type": "module",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/dot.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./loader": {
			"import": {
				"types": "./dist/loader/index.d.ts",
				"default": "./dist/loader/index.js"
			}
		}
	},
	"scripts": {
		"build:tsc": "tsc",
		"build:api": "api-extractor run --local",
		"dev": "tsc --watch",
		"clean": "rm -rf dist/ docs/reference/ reports/ temp/ .stryker-tmp/ tsdoc-metadata.json",
		"test:eslint": "eslint",
		"test:api": "api-extractor run",
		"test:tsc:experimental": "tsc --experimentalDecorators true --noEmit --incremental false",
		"test:jest:tc39": "DECORATOR_TYPE=tc39 NODE_OPTIONS=\"$NODE_OPTIONS --no-warnings --experimental-vm-modules\" jest",
		"test:jest:experimental": "DECORATOR_TYPE=experimental NODE_OPTIONS=\"$NODE_OPTIONS --no-warnings --experimental-vm-modules\" jest",
		"stryker": "DECORATOR_TYPE=tc39 NODE_OPTIONS=\"$NODE_OPTIONS --no-warnings --experimental-vm-modules\" stryker run"
	},
	"author": "Matthew Scharley <matt@scharley.me>",
	"license": "MPL-2.0",
	"bugs": {
		"url": "https://github.com/mscharley/dot/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mscharley/dot.git"
	},
	"homepage": "https://github.com/mscharley/dot#readme",
	"funding": {
		"type": "individual",
		"url": "https://github.com/sponsors/mscharley"
	},
	"dependencies": {
		"async-mutex": "^0.5.0",
		"tslib": "*"
	},
	"devDependencies": {
		"@jest/globals": "30.2.0",
		"@microsoft/api-extractor": "7.52.15",
		"@mscharley/eslint-config": "4.2.19",
		"@mscharley/prettier-config": "3.1.7",
		"@stryker-mutator/core": "9.1.1",
		"@stryker-mutator/jest-runner": "9.1.1",
		"@stryker-mutator/typescript-checker": "9.1.1",
		"@swc/jest": "0.2.39",
		"@types/node": "22.18.8",
		"acorn": "8.15.0",
		"esbuild": "0.25.10",
		"generic-type-guard": "5.1.0",
		"jest": "30.2.0",
		"ts-node": "10.9.2",
		"typescript": "5.9.3"
	}
}
