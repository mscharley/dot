{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": ["github>mscharley/renovate-config"],
	"ignorePaths": ["**/examples/**"],
	"packageRules": [
		{
			"matchPackageNames": ["typescript"],
			"major": {
				"semanticCommitType": "fix"
			},
			"minor": {
				"semanticCommitType": "fix"
			}
		},
		{
			"matchDepNames": ["go", "golang"],
			"groupName": "golang"
		}
	]
}
